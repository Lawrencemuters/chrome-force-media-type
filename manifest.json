{
  "name": "Open as text/html",
  "description": "Adds an option to the context menu to open link as text/html.",
  "version": "0.1",
  "permissions": [
    "tabs", 
    "contextMenus",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "icons" : {
    "16" : "icon-16.png",
    "19" : "icon-19.png",
    "48" : "icon-48.png",
    "128" : "icon-128.png"
  },
  "background_page": "background.html"
}
